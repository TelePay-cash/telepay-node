{
  "name": "telepay-node",
  "version": "1.0.0",
  "description": "NodeJS SDK for the TelePay API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "npm run build -- --watch src",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TelePay-cash/telepay-node.git"
  },
  "keywords": [
    "telepay",
    "cryptocurrencies",
    "payment processing",
    "api",
    "PPaaS"
  ],
  "author": "Yohan Gonz√°lez Almaguer <yohan.gonzalez.almaguer@gmail.com> (https://github.com/zahirinatzuke)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TelePay-cash/telepay-node/issues"
  },
  "homepage": "https://github.com/TelePay-cash/telepay-node#readme",
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.3.1"
  },
  "dependencies": {
    "axios": "^0.27.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "files": [
    "dist/"
  ]
}
